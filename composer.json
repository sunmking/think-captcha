{
    "name": "sunmking/think-captcha",
    "description": "captcha package for thinkphp",
    "authors": [
        {
            "name": "sunmking",
            "email": "sunmking_skf@163.com"
        }
    ],
    "license": "Apache-2.0",
    "require": {
        "topthink/framework": "^6.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "sunmking\\captcha\\": "src/"
        },
        "files": [
            "src/helper.php"
        ]
    },
    "extra": {
        "think": {
            "services": [
                "sunmking\\captcha\\CaptchaService"
            ],
            "config":{
                "captcha": "src/config.php"
            }            
        }
    }
}
